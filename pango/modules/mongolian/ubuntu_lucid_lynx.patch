+			if (previous == other && next == letter)
+			{
+				properties[i] |= initial_p;
+				continue;
+			}
+
+			if (previous == letter && next == other)
+			{
+				properties[i] |= final_p;
+				continue;
+			}
+
+			if (previous == letter && next == letter)
+			{
+				properties[i] |= medial_p;
+				continue;
+			}
